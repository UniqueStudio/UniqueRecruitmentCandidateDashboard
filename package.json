{
  "name": "unique-recruitment-candidate-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "extract": "NODE_ENV=development lingui extract --overwrite",
    "compile": "lingui compile",
    "dev": "next dev",
    "build": "next build && next export",
    "start": "next start",
    "prebuild": "rimraf .next",
    "prepare": "husky install"
  },
  "dependencies": {
    "@lingui/core": "^3.8.10",
    "@lingui/detect-locale": "^3.8.10",
    "@lingui/react": "^3.8.10",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@reduxjs/toolkit": "^1.5.1",
    "clsx": "^1.1.1",
    "next": "10.1.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.0.3",
    "react-redux": "^7.2.3",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@lingui/cli": "^3.8.10",
    "@lingui/loader": "^3.8.10",
    "@lingui/macro": "^3.8.10",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-redux": "^7.1.16",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "babel-plugin-macros": "^3.0.1",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "lint-staged": ">=10",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.2.3",
    "webpack": "^5.31.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{css,scss,json}": "prettier --write"
  },
  "lingui": {
    "locales": [
      "zh",
      "en"
    ],
    "catalogs": [
      {
        "path": "src/locales/{locale}/messages",
        "include": [
          "src"
        ]
      }
    ],
    "format": "po"
  }
}
